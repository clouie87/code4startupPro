<div class="row">
  <div class="col m6 offset-m3 center">
    <h2>Sign up</h2>

    <%= link_to "Sign up with Google", user_omniauth_authorize_path(:google_oauth2), class: "btn red darken-1" %>

    <%= link_to "Sign up with Facebook", user_omniauth_authorize_path(:facebook), class: "btn blue darken-4" %>

    <%= link_to "Sign up with Github", user_omniauth_authorize_path(:github), class: "btn grey darken-2" %>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render partial: "shares/devisemes" %>

      <div class="input-field">
        <%= f.text_field :name, autofocus: true, :class => "validate", :placeholder => "name"%>
      </div>

      <div class="input-field">
        <%= f.email_field :email, :class => "validate", :placeholder => "email" %>
      </div>

      <div class="input-field">
        <%= f.password_field :password, autocomplete: "off", :class => "validate", :placeholder => "password" %>
      </div>
      
      <button class="btn waves-effect waves-light" type="submit" name="action">Sign Up</button>
      
    <% end %>

  </div>    
</div>



